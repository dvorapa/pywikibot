[report]
ignore_errors = True
skip_empty = True

exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover
    except ImportError
    except KeyboardInterrupt
    except OSError
    except pkg_resources
    except \w*ServerError
    except SyntaxError
    raise NotImplementedError
    raise unittest\.SkipTest
    self\.skipTest
    if .PYWIKIBOT_TEST_.*. in os.environ:
    if self\.mw_version < .+:
    @(abc\.)?abstractmethod
    @deprecated\([^\)]+\)
